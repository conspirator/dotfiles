# File system shortcuts
alias ls='ls -al'
alias .='cd ../'
alias ..='cd ../../'
alias ...='cd ../../../'

# Web-dev helpers
alias server='python -m SimpleHTTPServer'

# Git
alias g='git'
alias grma='git add -u'

# Bash/Prompt stuff
alias rebash="source ~/.bash_profile && source ~/.bashrc"
alias edit_bash="s ~/.bash_profile ~/.bashrc"
